{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android":{
        "buildType": "apk"
      },
      "env":{
        "EXPO_PUBLIC_API_URL": "https://api.rplsmeas.cloud/api/",
        "EXPO_PUBLIC_DONWLOAD_URL": "https://api.rplsmeas.cloud/"
      }
    },
    "production": {
      "autoIncrement": true,
      "android":{
        "buildType": "apk"
      },
      "env":{
        "EXPO_PUBLIC_API_URL": "https://api.rplsmeas.cloud/api/",
        "EXPO_PUBLIC_DONWLOAD_URL": "https://api.rplsmeas.cloud/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
